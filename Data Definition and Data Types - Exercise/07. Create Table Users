CREATE TABLE users(
id INT PRIMARY KEY NOT NULL UNIQUE AUTO_INCREMENT,
username VARCHAR(30) NOT NULL,
password VARCHAR(26) NOT NULL,
profile_picture TEXT,
last_login_time DATETIME,
is_deleted  BOOLEAN
);
